{
	"manifest_version": 2,

	"name": "TwitchPreview",
	"description": "Allows previews of twitch channels from another stream's page",
	"version": "1.0",
	// "background": {
	// 	"scripts": ["preview.js"]
	// },
	"content_scripts": [
	{
		"matches": ["*://*.twitch.tv/*"],
		"js": ["jquery.min.js", "preview.js"]
	}],

  	"browser_action": {
    	"default_icon": "icon.png"
  	}
}