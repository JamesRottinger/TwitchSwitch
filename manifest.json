{
	"manifest_version": 2,

	"name": "TwitchSwitch",
	"description": "Allows previews of twitch channels from another stream's page",
	"version": "1.1.0",
	"content_scripts": [
	{
		"matches": ["*://*.twitch.tv/*"],
		"js": ["libs/jquery.min.js", "libs/scriptTagContext.js", "libs/embedly.min.js", "preview.js"]
	}],
        "icons": { "16" : "icons/icon_16.png",
                   "48" : "icons/icon_48.png",
                   "128": "icons/icon_128.png" },
  	"browser_action": {
            "default_icon": { "19": "icons/icon_19.png",
                              "38": "icons/icon_38.png" },
            "default_title": "TwitchSwitch"
  	},
  	"permissions": [
        "*://*.embed.ly/*",
        "https://api.github.com/users/Rob--W*"
    ]
}
