{
	"manifest_version": 2,

	"name": "TwitchSwitch",
	"description": "Allows previews of twitch channels from another stream's page",
	"version": "1.0",
	"content_scripts": [
	{
		"matches": ["*://*.twitch.tv/*"],
		"js": ["jquery.min.js", "scriptTagContext.js", "embedly.min.js", "preview.js"]
	}],

  	"browser_action": {
    	"default_icon": "icon.png"
  	},
  	"permissions": [
        "*://*.embed.ly/*",
        "https://api.github.com/users/Rob--W*"
    ]
}